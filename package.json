  "scripts": {
    "build": "webpack --config webpack/webpack.prod.js",
    "start": "webpack --config webpack/webpack.dev.js && npm run copy:html && babel src/server.js --out-file src/server-transpiled.js && node src/server-transpiled.js",
    "start:dev": "npm start && test:watch",
    "test:watch": "npm run test -- --watch",
    "test": "NODE_ENV=test mocha --require testSetup.js --compilers ts:ts-node/register,tsx:ts-node/register --no-deprecation  src/**/*.test.ts src/**/*.test.tsx",
    "copy:html": "cp src/index.html dist/",
    "copy:images": "cp assets/images/* dist/",
    "clean:dist": "rm -rf ./dist && mkdir dist"
  },
