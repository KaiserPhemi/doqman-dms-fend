{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "allowJs": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["./src/**/*"],
  "exclude": ["./node_modules"]
}
